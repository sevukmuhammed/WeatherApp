<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="textBackground" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:textColor">#000000</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="colorAccent">#9EB9D3</item>
        <item name="android:background">@drawable/textbackground</item>
    </style>
    <style name="splash" parent="Theme.AppCompat.DayNight.NoActionBar">

    </style>

</resources>
